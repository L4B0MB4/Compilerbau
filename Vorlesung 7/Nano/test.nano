Integer fibrek(Integer n)
{
   if(n<=1)
   {
      return 1;
   }
   return fibrek(n-1)+fibrek(n-2);
}

Integer fibit(Integer n)
{
   Integer tmp, f0, f1;

   f0 = 1;
   f1 = 1;

   while(n > 0)
   {
      tmp = f1+f0;
      f0 = f1;
      f1 = tmp;
      n=n-1;
   }
   return f0;
}


